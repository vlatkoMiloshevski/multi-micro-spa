{
  "name": "client",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "apimocker": "bnr apimocker",
    "local": "concurrently -p \"[{name}]\" -n \"api,web\" -c \"green.bold,yellow.bold\" \"bnr apimocker\" \"bnr serve:local\"",
    "dev": "bnr serve:dev",
    "prod": "bnr serve:prod",
    "demo": "bnr serve:demo",
    "start": "ng serve",
    "build-local": "bnr build:local",
    "build-dev": "bnr build:dev",
    "build-prod": "bnr build:prod",
    "build-demo": "bnr build:demo",
    "build-aws": "bnr build:aws",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "start-prebuy-pacing": "webpack-dev-server --env dev --port 9099",
    "start-prebuy-pacing:prod": "webpack-dev-server --env prod --port 9099",
    "build-prebuy-pacing": "webpack --env prod --progress",
    "build-prebuy-pacing:prod": "webpack -p --env prod --progress"
  },
  "betterScripts": {
    "build:local": {
      "command": "ng build -c local"
    },
    "build:dev": {
      "command": "ng build -c dev"
    },
    "build:prod": {
      "command": "ng build --prod"
    },
    "build:demo": {
      "command": "ng build -c demo"
    },
    "build:aws": {
      "command": "ng build -c aws"
    },
    "serve:local": {
      "command": "ng serve -c local"
    },
    "serve:dev": {
      "command": "ng serve -c dev"
    },
    "serve:prod": {
      "command": "ng serve --prod"
    },
    "serve:demo": {
      "command": "ng serve --demo"
    },
    "serve:aws": {
      "command": "ng serve -c aws"
    },
    "apimocker": {
      "command": "apimocker -c ./apimocker/config.json"
    }
  },
  "private": true,
  "dependencies": {
